# platform :ios, '11.0'
source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/surenr/public.cocopods'
target 'Boilerplate-Udf-IOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'ReSwift'
  pod 'PromiseKit'
  pod 'Alamofire'
  pod 'UdfBase'
  # Pods for Boilerplate-Udf-IOS

  target 'Boilerplate-Udf-IOSTests' do
    use_frameworks!
    # Pods for testing
    pod 'Quick'
    pod 'Nimble', :inhibit_warnings => true
  end

end
